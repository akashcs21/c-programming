#include<stdio.h>
int main()
{
  char alphabet;
  printf("the upper case alphabet are:");
  alphabet=65;
  while(alphabet<=90)
  {
      printf("%c ",alphabet);
      alphabet++;
  } 
  printf("\n");
  printf("the lowercase alphabet are :");
  alphabet=97;
  while(alphabet<=122)
  {
      printf("%c ",alphabet);
      alphabet++;
  }
 
   
   
   
}
